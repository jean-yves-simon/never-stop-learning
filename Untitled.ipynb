    "# calcul du minimum d'une liste\n",
    "\n",
    "def Minimum(L) :\n",
    "    min_courant = L[0]\n",
    "    for v in L :\n",
    "        if v < min_courant :\n",
    "            min_courant = v\n",
    "    return min_courant\n",
    "\n",
    "print( Minimum([4,88,9,-11,-100]) )\n"
   ]
